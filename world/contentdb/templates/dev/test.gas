[dev,t:template,n:test_potion_mana_channel]
{
	specializes	= potion;

	[aspect]
	{
		[textures]
		{
			0 =	b_i_glb_bottle-red;
		}
	}

	[common]
	{
		screen_name					= "[TEST] Potion of Mana Channel [TEST]";
		is_pcontent_allowed	= false;

		[template_triggers]
		{
			[*]
			{
				condition* = receive_world_message("we_constructed");
				action*	   = call_sfx_script("indicator_magic");
			}
		}
	}

	potion:inventory_icon_2	= b_gui_ig_i_it_potion-health;

	[magic]
	{
		[enchantments]
		{
			[*]	{
				alteration			= alter_mana;
				effect_script		= mana_trail;
				description			= mana channel;
				is_source_transfer	= true;
				is_target_transfer	= false;

				value				= 1	+ 5.5*(#nature_magic+1);
				max_value			= 11 + 5.5*(#nature_magic+1);
				duration			= #int + (1+2.5*(#nature_magic+1));
				frequency			= 0.10;
				transfer_efficiency	= ((#int/100.0)+(#mana_recovery_unit/#mana_recovery_period)) * (#nature_magic+1);

				is_enhancement		= true;
				is_permanent		= true;
			}

		}
	}
}

